<% provide :title_web, t("title1") %>
<!DOCTYPE html>
<html>
  <head>
    <title><%= yield :title %> | <%= yield :title_web %></title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag "application",
      media: "all","data-turbolinks-track": "reload" %>
    <%= javascript_include_tag "application",
      "data-turbolinks-track": "reload" %>
  </head>
  <body>
    <%= yield %>
    <%= link_to t(".en"), locale: :en %>
    <%= link_to t(".vi"), locale: :vi %>
  </body>
</html>
